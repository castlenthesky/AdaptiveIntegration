{
  "name": "template-typescript-basic",
  "version": "1.0.0",
  "description": "A boilerplate for developing applications using typescript.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "tsc-watch --onSuccess \"nodemon ./dist/index.js\"",
    "start": "dist/index.js",
    "delete": "",
    "test": "jest --silent --coverage",
    "test:watch": "jest --silent --coverage --watch --maxWorkers=1"
  },
  "author": "Brian Henson",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.1",
    "xml2js": "^0.4.23"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ]
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@types/xml2js": "^0.4.11",
    "jest": "^28.1.1",
    "nodemon": "^2.0.16",
    "ts-jest": "^28.0.5",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  }
}
